<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <title>MotionScript: Editor</title>
    <link rel="stylesheet" href="../Content/styles/main.css"/>
</head>
<body>
    <table>
        <tr>
            <th>
            </th>
            <th>
                Название
            </th>
        </tr>
        @Each
        <tr>
            <td>
                <a href="/edit/@Current.Id">Edit</a>
            </td>
            <td>
                @Current.Name
            </td>
        </tr>
        @EndEach

    </table>

    <p>
        <a href="/create">Create NEW</a>
    </p>
</body>
</html>
